@model IEnumerable<AngelOneAdmin.Models.ManualTrading>

@{
    ViewBag.Title = "Manual Trading Data";
    var counter = 0;
}


<style>
    .table {
        font-size:14px;
    }
</style>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">ClientId</th>
            <th scope="col">Trading Symbol</th>
            <th scope="col">Option Type</th>
            <th scope="col">Transaction Type</th>
            <th scope="col">Price</th>
            <th scope="col">Added Date</th>
            <th scope="col">Lot Size</th>
            <th scope="col">Is Executed</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            counter = counter + 1;
        <tr>
            <td>@item.id</td>
            <td>@item.clientId</td>
            <td>@item.tradingSymbol</td>
            <td>@item.optionType</td>
            <td>@item.transactionType</td>
            <td>@item.price</td>
            <td>@item.addedDate</td>
            <td>@item.lotsize</td>
            <td>@item.IsExecute</td>
        </tr>
        }
        
    </tbody>
</table>
